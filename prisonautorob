game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["Prisoners to Criminals"].CFrame
wait(3)

local Partt = Instance.new("Part")
Partt.Parent = game:GetService("Workspace")
Partt.Anchored = true
Partt.Name = "JewelryEnd"
Partt.Position = Vector3.new(444, 290, 1585)

local Partt11 = Instance.new("Part")
