Partt4.Name = "GasRob"
Partt4.Position = Vector3.new(960, 281, 54)

while wait(2) do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["Prisoners to Criminals"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["JewelryStart"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["JewelryEnd"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["BankStart"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["BankEnd"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["PostalStart"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["PostalEnd"].CFrame
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["GasRob"].CFrame
wait(2)
for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                   if v:FindFirstChild("ProximityPrompt") then
                       fireproximityprompt(v.ProximityPrompt)
           end
       end
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace["Chest"].CFrame
wait(2)
for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                   if v:FindFirstChild("ProximityPrompt") then
                       fireproximityprompt(v.ProximityPrompt)
           end
       end
end
